{
  "name": "oauth-app",
  "version": "1.0.0",
  "description": "OAuth application with user profile",
  "main": "server.js",
  "scripts": {
    "server-install": "npm install",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "yarn --cwd client serve",
    "dev": "concurrently \"yarn server\" \"yarn client\""
  },
  "author": "Shashank S Shetty",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.2.2",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "mongoose": "^5.6.10",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.1"
  }
}
